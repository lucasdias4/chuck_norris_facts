<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search_item_layout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/item_layout_height_search_fragment">

    <TextView
        android:id="@+id/search_item_text"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/item_text_margin_search_fragment"
        android:fontFamily="@font/roboto_regular"
        android:paddingStart="@dimen/item_text_padding_start_search_fragment"
        android:paddingEnd="@dimen/item_text_padding_end_search_fragment"
        android:textColor="@color/darkBlack"
        android:textSize="@dimen/item_text_size_search_fragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="search" />
</androidx.constraintlayout.widget.ConstraintLayout>